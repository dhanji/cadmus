;; Full codebase audit (Racket): 20-step pipeline
(define (codebase-audit-racket (path : Dir))
  (walk_tree)
  (find_matching :pattern "*.rs")
  (filter :pattern "test")
  (sort_by "name")
  (unique)
  (filter :pattern "integration")
  (sort_by "name")
  (unique)
  (filter :pattern "nl")
  (sort_by "name")
  (unique)
  (filter :pattern "stress")
  (sort_by "name")
  (unique)
  (filter :pattern "pipeline")
  (sort_by "name")
  (unique)
  (sort_by "name")
  (unique)
  (count))
